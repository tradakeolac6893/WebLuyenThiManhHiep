<%@ Page Title="" Language="C#" MasterPageFile="~/Master/LayOutSingle.Master" AutoEventWireup="true" CodeBehind="Login.aspx.cs" Inherits="LuyenThiHiep.Login" %>

<asp:Content ID="Content1" ContentPlaceHolderID="head" runat="server">
</asp:Content>
<asp:Content ID="Content2" ContentPlaceHolderID="ContentPlaceHolder1" runat="server">
    <!--single start here-->
    <div class="single">
        <div class="container">
            <div class="col-md-8 ">
                
                    <div class="single-bottom">
                        <h3>Đăng Nhập tài khoản</h3>
                        <div class="col-md-10 comment">
                            <input id="txtUserName" type="text" runat="server" placeholder="Tài khoản đăng nhập" />
                        </div>
                        <div class="col-md-10 comment">
                            <input id="txtPass" type="password" runat="server" placeholder="Mật Khẩu" />
                        </div>
                        <div class="col-md-5 comment">
                            <input id="btnlogin" style="width: 95%; text-align: center" type="submit" runat="server" value="Đăng nhập" onserverclick="btnLogin_Click" />
                        </div>
                        <div class="col-md-5 comment">
                            <input id="btnDangki" style="width: 95%; text-align: center" runat="server" type="submit" value="Tạo tài khoản" onserverclick="btnRegister_Click" />
                        </div>
                    </div>
            </div>
            <div class="col-md-3 categories-grid">
            </div>
            <div class="clearfix"></div>

        </div>
    </div>
</asp:Content>
